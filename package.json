{
  "name": "functions-for-platforms",
  "version": "1.0.0",
  "description": "Node implementation of the workers-for-platform service from CloudFlare",
  "scripts": {
    "build": "npm run barrels && tsc --project tsconfig.compile.json",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "npm run barrels && tsnd --inspect --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "prisma:migrate": "npx prisma migrate dev --name init",
    "prisma:generate": "npx prisma generate",
    "test:lint": "eslint '**/*.{ts,js}'",
    "test:lint:fix": "eslint '**/*.{ts,js}' --fix"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.88.0",
    "@prisma/client": "^3.14.0",
    "@tsed/ajv": "^6.114.9",
    "@tsed/common": "^6.114.9",
    "@tsed/core": "^6.114.9",
    "@tsed/di": "^6.114.9",
    "@tsed/engines": "^6.114.9",
    "@tsed/exceptions": "^6.114.9",
    "@tsed/json-mapper": "^6.114.9",
    "@tsed/logger": "^6.1.2",
    "@tsed/logger-file": "^6.1.2",
    "@tsed/passport": "^6.114.9",
    "@tsed/platform-cache": "^6.114.9",
    "@tsed/platform-exceptions": "^6.114.9",
    "@tsed/platform-express": "^6.114.9",
    "@tsed/platform-log-middleware": "^6.114.9",
    "@tsed/platform-middlewares": "^6.114.9",
    "@tsed/platform-params": "^6.114.9",
    "@tsed/platform-response-filter": "^6.114.9",
    "@tsed/platform-views": "^6.114.9",
    "@tsed/prisma": "^6.114.9",
    "@tsed/schema": "^6.114.9",
    "@tsed/swagger": "^6.114.9",
    "ajv": "^8.11.0",
    "barrelsby": "^2.3.4",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "dotenv-expand": "^8.0.3",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.1",
    "method-override": "^3.0.0",
    "passport": "^0.5.2"
  },
  "devDependencies": {
    "@tsed/cli": "^3.20.15",
    "@tsed/cli-plugin-eslint": "3.20.15",
    "@tsed/cli-plugin-passport": "3.20.15",
    "@tsed/cli-plugin-prisma": "3.20.15",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/method-override": "^0.0.32",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.33",
    "@types/passport": "^1.0.7",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^4.0.0",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
  },
  "main": "src/index.ts",
  "author": "",
  "license": "ISC",
  "tsed": {
    "packageManager": "npm",
    "convention": "default",
    "architecture": "default"
  }
}
